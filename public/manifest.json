{
  "manifest_version": 3,
  "name": "Keep Problems",
  "description": "Save problem with link, name, difficulty, code and notes.",
  "version": "0.1.1",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_icon": "logo.png",
    "default_title": "Keep Problems",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage"
  ]
}